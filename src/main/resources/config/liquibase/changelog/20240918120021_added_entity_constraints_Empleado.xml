<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.6.xsd">
    <!--
        Added the constraints for entity Empleado.
    -->
    <changeSet id="20240918120021-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="persona_id"
                                 baseTableName="empleado"
                                 constraintName="fk_empleado__persona_id"
                                 referencedColumnNames="id"
                                 referencedTableName="persona"/>

        <addForeignKeyConstraint baseColumnNames="establecimiento_id"
                                 baseTableName="empleado"
                                 constraintName="fk_empleado__establecimiento_id"
                                 referencedColumnNames="id"
                                 referencedTableName="establecimiento"/>
    </changeSet>
</databaseChangeLog>
